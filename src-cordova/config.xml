<?xml version='1.0' encoding='utf-8'?>
<widget id="org.mteb.cl" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>cApp</name>
    <description>cicada for users</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="bdapp:*" />
    <platform name="android">
        <icon density="ldpi" src="images/icon/android/logo.png" />
        <icon density="mdpi" src="images/icon/android/logo.png" />
        <icon density="hdpi" src="images/icon/android/logo.png" />
        <icon density="xhdpi" src="images/icon/android/logo.png" />
        <icon density="xxhdpi" src="images/icon/android/logo.png" />
        <splash density="land-hdpi" src="images/screen/android/splash-land-hdpi.png" />
        <splash density="land-ldpi" src="images/screen/android/splash-land-ldpi.png" />
        <splash density="land-mdpi" src="images/screen/android/splash-land-mdpi.png" />
        <splash density="land-xhdpi" src="images/screen/android/splash-land-xhdpi.png" />
        <splash density="port-hdpi" src="images/screen/android/splash-port-hdpi.png" />
        <splash density="port-ldpi" src="images/screen/android/splash-port-ldpi.png" />
        <splash density="port-mdpi" src="images/screen/android/splash-port-mdpi.png" />
        <splash density="port-xhdpi" src="images/screen/android/splash-port-xhdpi.png" />
        <allow-intent href="market:*" />
        <allow-intent href="bdapp:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <allow-intent href="baidumap:*" />
    </platform>
    <allow-navigation href="about:*" />
</widget>
